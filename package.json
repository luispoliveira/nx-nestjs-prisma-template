  "devDependencies": {
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@nestjs/schematics": "10.1.2",
    "@nestjs/testing": "10.3.10",
    "@nx/eslint": "19.4.3",
    "@nx/eslint-plugin": "19.4.3",
    "@nx/jest": "19.4.3",
    "@nx/js": "19.4.3",
    "@nx/nest": "19.4.3",
